spring:
  application:
    name: rating-service
server:
  port: 8084

eureka:

  client:
    serviceUrl:
      defaultZone: ${EUREKA_URI:http://localhost:8761/eureka}
    region: default  
    registryFetchIntervalSeconds: 5
    registerWithEureka: true
    fetchRegistry: true

  instance:
    preferIpAddress: true
 
    
    
